{
    "name": "vankosoft/thruway-bundle",
    "type": "symfony-bundle",
    "description": "VankoSoft Symfony Application Extension - WebSockets (WAMP2) integration",
    "keywords": [
        "WebSockets",
        "WAMP",
        "WAMP2",
        "Autobahn",
        "Sockets",
        "Symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "David Dan",
            "email": "davidwdan@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Matt Bonneau",
            "email": "matt@bonneau.net",
            "role": "Developer"
        },
        {
            "name": "Ivan I. Atanasov",
            "email": "i.atanasov77@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vankosoft\\ThruwayBundle\\": "lib/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/finder": "^6.4",
        "voryx/thruway": "~0.6",
        "symfony/serializer": "^6.4",
        "react/child-process": "~0.6",
        "symfony/event-dispatcher": "^6.4",
        "thruway/pawl-transport": "^0.5.1"
    },
    "suggest": {
        "vankosoft/application": "This a Required Core Library.",
        "cboden/ratchet": "If you want to create WebSocket Server.",
        "react/zmq": "If you have 'zmq' PHP extension Loaded you can install this to create WebSocket Clients."
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "symfony/doctrine-bridge": "<2.7"
    },
    "minimum-stability" : "dev"
}
